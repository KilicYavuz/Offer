$color_primary: {{settings.color_primary}};

.container{
  @include make-padding();
  @include make-container-max-widths();
}

.container-fluid {
  @include make-padding();
}

@media (min-width: 1200px) {
  .container-fluid.wide {
    padding-left: 30px;
    padding-right: 30px;
  }
}

@media (min-width: 1600px) {
  .container-fluid.wide {
    padding-left: 70px;
    padding-right: 70px;
  }
  .order-xxl-first {
    -ms-flex-order: -1;
    order: -1;
  }

  .order-xxl-last {
    -ms-flex-order: 13;
    order: 13;
  }

  .order-xxl-1 {
    -ms-flex-order: 1;
    order: 1;
  }
}

@media (min-width: 1920px) {
  .container-fluid.wide {
    max-width: 1920px;
    margin-left: auto;
    margin-right: auto;
  }
}

@media (max-width: 768px) {
  .container-fluid {
    padding-left: 15px;
    padding-right: 15px;
  }
  .container {
    padding-left: 15px;
    padding-right: 15px;
    .container {
      @include make-padding();
    }
  }
}

.row {
  @include make-row();
}

.row.row-small {
  @include make-row-small();
}

.no-gutters {
  margin-right: 0;
  margin-left: 0;
  > .col,
  > [class*="col-"] {
    padding-right: 0;
    padding-left: 0;
  }
}

[class^="col-"], [class*=" col-"], .col, .col-auto{
  @include make-padding();
}

.p-small[class^="col-"], .p-small[class*=" col-"], .p-small.col, .p-small.col-auto{
  @include make-padding-small();
}

.m--10x-lr{
  margin-left: -10px;
  margin-right: -10px;
}

.p-10x-lr{
  @include make-padding-small();
}

@media (min-width: 320px) {
}

@media (min-width: 576px) {
  .m-sm--with-base-lr{
    @include make-row();
  }
  .p-sm-with-base-lr{
    @include make-padding();
  }
}

@media (min-width: 768px) {
  .p-md-with-base-lr{
    @include make-padding();
  }
  .p-md-with-base-lr-i{
    @include make-padding-i();
  }
  [dir="ltr"]{
    .p-md-with-base-l{    
      padding-left: ($grid-gutter-width / 2) !important;
    }
    .p-md-with-base-r{    
      padding-right: ($grid-gutter-width / 2) !important;
    }
  }
  [dir="rtl"]{
    .p-md-with-base-l{
      padding-right: ($grid-gutter-width / 2) !important;
    }
    .p-md-with-base-r{    
      padding-left: ($grid-gutter-width / 2) !important;
    }
  }
}

