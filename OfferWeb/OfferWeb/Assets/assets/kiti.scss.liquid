// GRID
$grid-columns: 12 !default;
$grid-gutter-width: 20px !default;
$grid-gutter-width-small: 20px !default;

$grid-breakpoints: ({{settings.breakpoints}}) !default;
$container-max-widths: ({{settings.max_width}}) !default;

@mixin make-padding() {
  padding-right: ($grid-gutter-width / 2);
  padding-left: ($grid-gutter-width / 2);
}

@mixin make-padding-i() {
  padding-right: ($grid-gutter-width / 2) !important;
  padding-left: ($grid-gutter-width / 2) !important;
}

@mixin make-padding-small() {
  padding-right: ($grid-gutter-width-small / 2);
  padding-left: ($grid-gutter-width-small / 2);
}

@mixin make-container-max-widths($max-widths: $container-max-widths, $breakpoints: $grid-breakpoints) {
  $i : 0;
  @each $breakpoint in $breakpoints {
    $i : $i + 1;
    @if $breakpoint != 0{
      @media (min-width: $breakpoint) {
        max-width: nth($max-widths, $i);
      }
    } @else {
      max-width: nth($max-widths, $i);
    }
  }
}

@mixin make-container-max-widths($max-widths: $container-max-widths, $breakpoints: $grid-breakpoints) {
  $i : 0;
  @each $breakpoint in $breakpoints {
    $i : $i + 1;
    @if $breakpoint != 0{
      @media (min-width: $breakpoint) {
        max-width: nth($max-widths, $i);
      }
    }
  }
}

@mixin make-row() {
  margin-right: ($grid-gutter-width / -2);
  margin-left: ($grid-gutter-width / -2);
}

@mixin make-row-small() {
  margin-right: ($grid-gutter-width-small / -2);
  margin-left: ($grid-gutter-width-small / -2);
}


