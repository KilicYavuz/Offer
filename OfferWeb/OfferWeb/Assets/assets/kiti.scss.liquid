    @if $i == 0{
      .mt#{$screen}-auto {
        margin-top: auto;
      }
      .mr#{$screen}-auto {
        margin-right: auto;
      }
      .mb#{$screen}-auto {
        margin-bottom: auto;
      }
      .ml#{$screen}-auto {
        margin-left: auto;
      }
      .mx#{$screen}-auto {
        margin-right: auto;
        margin-left: auto;
      }
      .pt#{$screen}-auto {
        padding-top: auto;
      }
      .pb#{$screen}-auto {
        padding-bottom: auto;
      }
    }
    @if $i <= 5{
      .m#{$screen}-#{$i * $num} {
        margin: #{$i * $num}rem;
      }
      .p#{$screen}-#{$i * $num} {
        padding: #{$i * $num}rem;
