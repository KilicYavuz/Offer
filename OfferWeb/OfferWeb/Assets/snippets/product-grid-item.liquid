<!-- product-grid-item.liquid -->
{%- liquid
  assign compare_at_price = product.compare_at_price 
  assign price = product.price 
  assign price_varies = product.price_varies 
  assign available = product.available 
  if st_card[1] == "true"
    unless product.tags contains 'notnew'
      assign on_new = false
      assign now_date_int = "now" | date: "%j" | plus: 0
      assign now_year_int = "now" | date: '%Y' | plus: 0
      assign pub_date_int = product.published_at | date: "%j" | plus: 0
      assign pub_year_int = product.published_at | date: '%Y' | plus: 0
      if now_year_int == pub_year_int
        assign new_day_int = st_card[2] | default: 30 | plus: 0
        assign check_new = now_date_int | minus: pub_date_int
        if check_new >= 0 and check_new <= new_day_int
          assign on_new = true
        endif
      else
        assign new_day_int = st_card[2] | default: 30 | plus: 366
        assign check_new = now_date_int | plus: 366
        if check_new <= new_day_int
          assign on_new = true
        endif
      endif
    endunless
  endif
  assign on_sale = false
  if compare_at_price > price 
    assign on_sale = true
  endif
  assign sold_out = true
  assign preOrder = false
  unless available == false
    assign sold_out = false
  endunless
  if product.selected_or_first_available_variant.incoming
    assign preOrder = true 
  endif
  unless grid_layout
    assign grid_layout = st_card[4]
  endunless
  unless masonry
    assign masonry = false
    if st_card[7] == 'normal'
      assign masonry = 'none'
    endif
  endunless
  unless block_id
    assign block_id = block.id
  endunless
  unless section_id
    assign section_id = section.id
  endunless
-%}
<div class="product-item">
  <div class="product-wrapper">
    <div class="product {{st_card[10]}} product-inner{% unless product.has_only_default_variant or st_card[11] == 'true' and product.variants.size == 1 or st_card[12] == 'false' %} ktlz{%endunless%}" data-href="{{product.handle}}"{%unless product.has_only_default_variant%} data-show-name="{{ st_card[13] }}" {%if product.options.size >= 1%} data-show-only-first-option="{{st_card[22]}}"{%endif%}{%endunless%}>
